    "Net-(C5-Pad1)" "/OUT_BUF" "Net-(C6-Pad2)" "Net-(C7-Pad1)" "/PV_3" "/IN_B" "/IN_N" "Net-(P3-Pad1)"
    "/PV_2" /BAT+ "/IN_EQ" "/PB1_2" "/PB2_2" /V+ "Net-(Q3-Pad1)" "Net-(Q4-Pad1)"
    "Net-(U1-Pad1)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
